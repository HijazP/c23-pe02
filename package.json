{
  "name": "pic_desa-api",
  "version": "1.0.0",
  "description": "API untuk PIC Desa",
  "main": "index.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn ./src/index.ts",
    "pm2": "tsc && pm2 start pm2.config.js",
    "seed:desa": "ts-node ./prisma/desaProblemSeed.ts",
    "seed:course": "ts-node ./prisma/courseModulSeed.ts",
    "migrate": "npx prisma migrate dev --preview-feature --skip-generate --name init",
    "generate": "npx prisma generate",
    "compile": "tsc"
  },
  "author": "Bigstem",
  "license": "ISC",
  "dependencies": {
    "@hapi/hapi": "^21.3.2",
    "@hapi/inert": "^7.1.0",
    "@hapi/jwt": "^3.2.0",
    "@prisma/client": "^4.15.0",
    "@types/hapi": "^18.0.8",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.1.3",
    "pm2": "^5.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "prisma": "^4.15.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  }
}
